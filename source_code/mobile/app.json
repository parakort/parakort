{
  "app": {
    "server": "http://172.20.10.3:3001",
    "welcome_msg": "Welcome! You have been granted some free tokens! Please view the Preferences tab to learn how to use the app.",
    "theme": {
      "red": "#F9063C",
      "blue": "#849bff",
      "creme": "#FFFFF1",
      "grey": "#ccc"
    }
  },
  "expo": {
    "name": "The Clubhouse",
    "slug": "TheClubhouse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.builtbypeter.theclubhouse",

      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location is required to discover nearby players.",
        "NSLocationAlwaysUsageDescription": "Your location is required to discover nearby players."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },

      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "owner": "peterbuo",
    "extra": {
      "eas": {
        "projectId": "fcef904b-4430-4f5a-8698-61e6123aa623"
      }
    }
  }
}
